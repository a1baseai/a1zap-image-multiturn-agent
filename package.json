{
  "name": "ai-agent-file-context",
  "version": "2.0.0",
  "description": "AI agent system with file context support - Create intelligent agents that analyze documents and data using Claude and Gemini",
  "main": "server.js",
  "scripts": {
    "start": "dotenvx run -f .env -- node server.js",
    "dev": "dotenvx run -f .env -- nodemon server.js",
    "check": "dotenvx run -f .env -- node check-config.js",
    "upload": "dotenvx run -f .env -- node examples/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-agent-file-context.git"
  },
  "keywords": [
    "ai-agent",
    "claude",
    "gemini",
    "anthropic",
    "webhook",
    "file-analysis",
    "data-analysis",
    "document-ai",
    "csv-analysis",
    "a1zap",
    "chatbot"
  ],
  "author": "AI Agent Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@dotenvx/dotenvx": "^1.51.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
